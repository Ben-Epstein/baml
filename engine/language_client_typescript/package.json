{
  "name": "@boundaryml/baml",
  "version": "0.0.1",
  "description": "BAML typescript bindings (package.json)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BoundaryML/baml.git"
  },
  "license": "MIT",
  "keywords": [
    "napi-rs",
    "NAPI",
    "N-API",
    "Rust",
    "node-addon",
    "node-addon-api"
  ],
  "bin": "cli.js",
  "files": [
    "cli.js",
    "index.d.ts",
    "index.js"
  ],
  "main": "index.js",
  "napi": {
    "name": "baml",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-pc-windows-msvc",
        "aarch64-unknown-linux-gnu",
        "x86_64-apple-darwin",
        "x86_64-pc-windows-msvc",
        "x86_64-unknown-linux-gnu"
      ]
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "pnpm build:debug --release",
    "build:debug": "mkdir -p dist && napi build --platform",
    "format": "run-p format:biome format:rs format:toml",
    "format:biome": "biome --write .",
    "format:rs": "cargo fmt",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "echo no tests implemented",
    "version": "napi version"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@napi-rs/cli": "^2.16.4",
    "@types/node": "^20.12.11",
    "npm-run-all2": "^6.1.2",
    "ts-node": "^10.9.2"
  },
  "packageManager": "pnpm@9.0.6"
}