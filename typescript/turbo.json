{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "watch": {
      "dependsOn": ["^watch"],
      "outputs": ["dist/**"]
    },
    "@gloo-ai/baml-schema-wasm#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "@gloo-ai/baml-language-server#build": {
      "dependsOn": ["@gloo-ai/baml-schema-wasm#build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "lint": {},
    "clean": {}
  }
}
